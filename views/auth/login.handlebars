<div class="container" id="divLogin">
  <form action="/login" method="post" id="methodLogin">
    <h1 id="textEntrar">Entrar</h1>
    <div id="divCPF">
      <label for="cpf" id="cpfText">CPF</label>
      <input type="text" id="cpf" name="cpf" pattern="\d{11}" title="O CPF deve ter 11 dÃ­gitos" required />
    </div>
    <div id="divSenha">
      <label for="password" id="senhaText">Senha</label>
      <input type="password" id="password" name="password" />
    </div>
    <button type="submit" id="entrar" class="btn btn-primary">Entrar</button>
    <a id="botaoCadastrarref" href="/cadastro">Cadastrar-se</a>
  </form>
</div>
