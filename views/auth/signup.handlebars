<div class="container" id="divCadastro">
  <form action="/cadastro" method="post" id="methodCadastro">
    <h1 id="textCad">Cadastre-se</h1>
    <div id="divCPF">
      <label for="cpf" id="textEmail">CPF</label>
      <input type="text" id="cpf" name="cpf" pattern="\d{11}" title="O CPF deve ter 11 dígitos" required />
    </div>
    <div id="divNome">
      <label for="nome" id="textNome">Nome</label>
      <input type="text" id="nome" name="nome" />
    </div>
    <div id="divSenha">
      <label for="password" id="textSenha">Senha</label>
      <input type="password" id="password" name="password" />
    </div>
    <div id="divConfSenha">
      <label for="confirmpassword" id="textConfSenha">Confirmação de Senha</label>
      <input type="password" id="confirmpassword" name="confirmpassword" />
    </div>
    <button type="submit" id="cadastrar" class="btn btn-primary">Cadastrar</button>
    <a id="botaoEntrarref" href="/login">Entrar</a>
  </form>
</div>

